{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    },
    "dev:ios": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "preview": {
      "dependsOn": ["build"]
    },
    "stories": {
      "outputs": []
    }
  },
  "globalDependencies": ["$SERVER_PORT"]
}
