{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "clear:dist": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "dev:android": {
      "cache": false
    },
    "dev:ios": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "preview": {
      "dependsOn": ["build"]
    },
    "stories": {
      "cache": false
    }
  },
  "globalDependencies": [
    "$SERVER_URL",
    "$SERVER_PORT",
    "$VITE_SERVER_URL",
    "$PLATFORM"
  ]
}
