    "preview": "turbo run preview",
    "stories": "ladle serve"
  },
  "devDependencies": {
    "@acima/eslint-config-custom": "*",
    "@acima/tsconfig": "*",
    "@ladle/react": "^2.4.2",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "prettier": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "turbo": "latest",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
