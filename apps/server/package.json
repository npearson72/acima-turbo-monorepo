{
  "name": "@acima/server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && node build.js",
    "clear:dist": "rm -rf dist",
    "predev": "node build.js",
    "dev": "concurrently 'node build.js --watch' 'sleep 0.1 && nodemon dist/index.js'",
    "lint": "eslint src --quiet",
    "preview": "node --enable-source-maps dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@acima/eslint-config-custom": "*",
    "@acima/tsconfig": "*",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.15",
    "concurrently": "^7.4.0",
    "esbuild": "^0.15.7",
    "nodemon": "^2.0.20",
    "prisma": "^4.3.1",
    "typescript": "^4.8.2",
    "yargs-parser": "^21.1.1"
  }
}
