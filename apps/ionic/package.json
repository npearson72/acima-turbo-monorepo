    "@capacitor/core": "^4.0.1",
    "@capacitor/haptics": "^4.0.1",
    "@capacitor/ios": "^4.0.1",
    "@capacitor/keyboard": "^4.0.1",
    "@capacitor/status-bar": "^4.0.1",
    "@emotion/react": "^11.10.4",
    "@iconify/react": "^4.0.0",
    "@ionic/react": "^6.2.0",
    "@ionic/react-router": "^6.2.0",
    "@mantine/core": "^5.3.0",
    "@mantine/hooks": "^5.3.0",
    "ionicons": "^6.0.3",
