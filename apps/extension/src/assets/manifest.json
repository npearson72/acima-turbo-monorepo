{
  "manifest_version": 3,
  "name": "Acima",
  "version": "0.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Acima"
  },
  "options_ui": {
    "page": "options.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
